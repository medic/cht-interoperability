      </itext>
      <instance>
        <claims_feedback id="claims_feedback" prefix="J1!claims_feedback!" delimiter="#" version="2025-08-27 11:29:10">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source>user</source>
            <source_id/>
            <op_claim_id/>
            <openimis_id/>
            <op_claim_uuid/>
            <contact>
              <_id/>
              <name/>
              <first_name/>
              <patient_id>0</patient_id>
              <date_of_birth/>
              <sex/>
              <parent>
                <_id/>
                <parent>
                  <_id/>
                  <parent>
                    <_id/>
                    <parent>
                      <_id/>
                    </parent>
                  </parent>
                </parent>
              </parent>
            </contact>
          </inputs>
          <patient_age_in_years/>
          <patient_uuid tag="hidden"/>
          <patient_id tag="hidden"/>
          <patient_name tag="hidden"/>
          <context_vars tag="hidden"/>
          <claim_feedback_group>
            <openimis_id_note/>
            <openimis_claim_id_note/>
            <care_rendered tag="hidden"/>
            <payment_asked/>
            <drug_prescribed/>
            <drug_received/>
            <assessment_rating/>
          </claim_feedback_group>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </claims_feedback>
      </instance>
      <instance id="contact-summary"/>
      <instance id="user-contact-summary"/>
      <bind nodeset="/claims_feedback/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/claims_feedback/inputs/source" type="string"/>
      <bind nodeset="/claims_feedback/inputs/source_id" type="string"/>
      <bind nodeset="/claims_feedback/inputs/op_claim_id" type="string"/>
      <bind nodeset="/claims_feedback/inputs/openimis_id" type="string"/>
      <bind nodeset="/claims_feedback/inputs/op_claim_uuid" type="string"/>
      <bind nodeset="/claims_feedback/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/claims_feedback/inputs/contact/name" type="string"/>
      <bind nodeset="/claims_feedback/inputs/contact/first_name" type="string"/>
      <bind nodeset="/claims_feedback/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/claims_feedback/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/claims_feedback/inputs/contact/sex" type="string"/>
      <bind nodeset="/claims_feedback/inputs/contact/parent/_id" type="string"/>
      <bind nodeset="/claims_feedback/inputs/contact/parent/parent/_id" type="string"/>
      <bind nodeset="/claims_feedback/inputs/contact/parent/parent/parent/_id" type="string"/>
      <bind nodeset="/claims_feedback/inputs/contact/parent/parent/parent/parent/_id" type="string"/>
      <bind nodeset="/claims_feedback/patient_age_in_years" type="string" calculate="floor( difference-in-months( ../inputs/contact/date_of_birth, today() ) div 12 )"/>
      <bind nodeset="/claims_feedback/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/claims_feedback/patient_id" type="string" required="true()" calculate="../inputs/contact/patient_id"/>
      <bind nodeset="/claims_feedback/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/claims_feedback/claim_feedback_group/openimis_id_note" readonly="true()" type="string"/>
      <bind nodeset="/claims_feedback/claim_feedback_group/openimis_claim_id_note" readonly="true()" type="string"/>
      <bind nodeset="/claims_feedback/claim_feedback_group/care_rendered" type="select1" required="true()"/>
      <bind nodeset="/claims_feedback/claim_feedback_group/payment_asked" type="select1" required="true()"/>
      <bind nodeset="/claims_feedback/claim_feedback_group/drug_prescribed" type="select1" required="true()"/>
      <bind nodeset="/claims_feedback/claim_feedback_group/drug_received" type="select1" required="true()"/>
      <bind nodeset="/claims_feedback/claim_feedback_group/assessment_rating" type="int" required="true()"/>
      <bind nodeset="/claims_feedback/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/claims_feedback/inputs">
      <label ref="jr:itext('/claims_feedback/inputs:label')"/>
      <group ref="/claims_feedback/inputs/contact">
        <input appearance="db-object" ref="/claims_feedback/inputs/contact/_id">
          <label ref="jr:itext('/claims_feedback/inputs/contact/_id:label')"/>
        </input>
        <group ref="/claims_feedback/inputs/contact/parent">
          <input ref="/claims_feedback/inputs/contact/parent/_id">
            <label ref="jr:itext('/claims_feedback/inputs/contact/parent/_id:label')"/>
          </input>
          <group ref="/claims_feedback/inputs/contact/parent/parent">
            <input ref="/claims_feedback/inputs/contact/parent/parent/_id">
              <label ref="jr:itext('/claims_feedback/inputs/contact/parent/parent/_id:label')"/>
            </input>
            <group ref="/claims_feedback/inputs/contact/parent/parent/parent">
              <input ref="/claims_feedback/inputs/contact/parent/parent/parent/_id">
                <label ref="jr:itext('/claims_feedback/inputs/contact/parent/parent/parent/_id:label')"/>
              </input>
              <group ref="/claims_feedback/inputs/contact/parent/parent/parent/parent">
                <input ref="/claims_feedback/inputs/contact/parent/parent/parent/parent/_id">
                  <label ref="jr:itext('/claims_feedback/inputs/contact/parent/parent/parent/parent/_id:label')"/>
                </input>
              </group>
            </group>
          </group>
        </group>
      </group>
    </group>
    <group ref="/claims_feedback/context_vars"/>
    <group appearance="field-list" ref="/claims_feedback/claim_feedback_group">
      <label ref="jr:itext('/claims_feedback/claim_feedback_group:label')"/>
      <input ref="/claims_feedback/claim_feedback_group/openimis_id_note">
        <label ref="jr:itext('/claims_feedback/claim_feedback_group/openimis_id_note:label')"/>
      </input>
      <input ref="/claims_feedback/claim_feedback_group/openimis_claim_id_note">
        <label ref="jr:itext('/claims_feedback/claim_feedback_group/openimis_claim_id_note:label')"/>
      </input>
      <select1 ref="/claims_feedback/claim_feedback_group/care_rendered">
        <label ref="jr:itext('/claims_feedback/claim_feedback_group/care_rendered:label')"/>
        <item>
          <label ref="jr:itext('/claims_feedback/claim_feedback_group/care_rendered/yes:label')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/claims_feedback/claim_feedback_group/care_rendered/no:label')"/>
          <value>no</value>
        </item>
      </select1>
      <select1 ref="/claims_feedback/claim_feedback_group/payment_asked">
        <label ref="jr:itext('/claims_feedback/claim_feedback_group/payment_asked:label')"/>
        <item>
          <label ref="jr:itext('/claims_feedback/claim_feedback_group/payment_asked/yes:label')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/claims_feedback/claim_feedback_group/payment_asked/no:label')"/>
          <value>no</value>
        </item>
      </select1>
      <select1 ref="/claims_feedback/claim_feedback_group/drug_prescribed">
        <label ref="jr:itext('/claims_feedback/claim_feedback_group/drug_prescribed:label')"/>
        <item>
          <label ref="jr:itext('/claims_feedback/claim_feedback_group/drug_prescribed/yes:label')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/claims_feedback/claim_feedback_group/drug_prescribed/no:label')"/>
          <value>no</value>
        </item>
      </select1>
      <select1 ref="/claims_feedback/claim_feedback_group/drug_received">
        <label ref="jr:itext('/claims_feedback/claim_feedback_group/drug_received:label')"/>
        <item>
          <label ref="jr:itext('/claims_feedback/claim_feedback_group/drug_received/yes:label')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/claims_feedback/claim_feedback_group/drug_received/no:label')"/>
          <value>no</value>
        </item>
      </select1>
      <input ref="/claims_feedback/claim_feedback_group/assessment_rating">
        <label ref="jr:itext('/claims_feedback/claim_feedback_group/assessment_rating:label')"/>
      </input>
    </group>
  </h:body>
</h:html>
